{
    "name": "zafranf/fnfw",
    "description": "(fn)FW | functional Framework - Simple framework for simple project",
    "type": "project",
    "require": {
        "php-imap/php-imap": "^3.0",
        "tijsverkoyen/css-to-inline-styles": "^2.2",
        "zafranf/fnfwcore": "^0.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Zafran Fadilah",
            "email": "surel@zafran.id"
        }
    ],
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('configuration-example.php', 'configuration.php');\""
        ],
        "post-create-project-cmd": [
            "chmod -R 777 storage/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
